<script type="text/template" data-name='filter'>
  <%% _(domains).each(function (count, domain) { %>
    <label class="checkbox">
      <input type="checkbox" value='<%%= domain %>' /><span class="domain">.<%%= domain %></span><span class="badge badge-info"><%%= count %></span>
    </label>
  <%% }); %>
</script>
